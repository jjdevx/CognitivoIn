{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "christofferok/laravel-emojione": "^2.1",
        "cviebrock/eloquent-sluggable": "^4.5",
        "doctrine/dbal": "^2.7",
        "fideloper/proxy": "^4.0",
        "florianv/laravel-swap": "^1.3",
        "lab404/laravel-impersonate": "^1.2",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/scout": "3.*",
        "laravel/tinker": "^1.0",
        "naoray/laravel-reviewable": "^1.1",
        "nasyrov/laravel-enums": "^1.1",
        "overtrue/laravel-follow": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/message": "^1.6",
        "pragmarx/countries": "^0.5.8",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-model-status": "^1.5",
        "spatie/laravel-tags": "^2.0",
        "tamayo/laravel-scout-elastic": "^3.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
